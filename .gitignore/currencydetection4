function result=plotbox(Target,Template,M);

[r1,c1]=size(Target);
[r2,c2]=size(Template);

[r,c]=max(M);
[r3,c3]=max(max(M));

i=c(c3);
j=c3;
result=Target;
for x=i:i+r2
   for y=j
       result(x,y)=0;
   end
end
for x=i:i+r2
   for y=j+c2
       result(x,y)=0;
   end
end
for x=i
   for y=j:j+c2
       result(x,y)=0;
   end
end
for x=i+r2
   for y=j:j+c2
       result(x,y)=0;
   end
end
